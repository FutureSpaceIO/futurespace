{
  "name": "futurespace",
  "version": "0.0.0",
  "private": true,
  "description": "Powered by Trek.js",
  "scripts": {
    "start": "babel-node server.js"
  },
  "dependencies": {
    "babel": "^5.*",
    "co": "^4.6.0",
    "koa-bodyparser": "^2.*",
    "koa-compress": "^1.*",
    "koa-generic-session": "^1.*",
    "koa-i18n": "^1.1.2",
    "koa-locale": "^1.0.0",
    "koa-logger": "^1.*",
    "koa-methodoverride": "^1.*",
    "koa-redis": "^1.*",
    "nunjucks": "^2.1.0",
    "pg": "^4.4.3",
    "transwarp": "github:trekjs/transwarp",
    "trek": "trekjs/trek#koa-2"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "browser-sync": "^2.*",
    "cssnano": "^3.3.2",
    "del": "^2.0.2",
    "gulp": "^3.*",
    "gulp-babel": "^5.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-load-plugins": "^1.*",
    "gulp-postcss": "^6.0.1",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.2",
    "gulp-watch": "^4.*",
    "node-sass": "^3.4.1"
  },
  "engines": {
    "node": ">= 4"
  },
  "babel": {
    "stage": 0,
    "optional": [
      "es7.asyncFunctions",
      "es7.classProperties"
    ],
    "blacklist": [
      "es6.arrowFunctions",
      "es6.blockScoping",
      "es6.constants",
      "es6.forOf",
      "es6.properties.computed",
      "es6.properties.shorthand",
      "es6.regex.sticky",
      "es6.regex.unicode",
      "es6.tailCall",
      "es6.templateLiterals",
      "regenerator"
    ]
  }
}
