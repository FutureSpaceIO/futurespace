{
  "name": "futurespace",
  "version": "0.0.0",
  "private": true,
  "description": "Powered by Trek.js",
  "scripts": {
    "start": "babel-node server.js"
  },
  "dependencies": {
    "babel": "^5.*",
    "co": "^4.6.0",
    "koa-bodyparser": "^2.*",
    "koa-compress": "^1.*",
    "koa-generic-session": "^1.*",
    "koa-logger": "^1.*",
    "koa-methodoverride": "^1.*",
    "koa-redis": "^1.*",
    "nunjucks": "^2.1.0",
    "transwarp": "github:trekjs/transwarp",
    "trek": "trekjs/trek#koa-2"
  },
  "devDependencies": {
    "browser-sync": "^2.*",
    "gulp": "^3.*",
    "gulp-load-plugins": "^1.*",
    "gulp-watch": "^4.*"
  },
  "engines": {
    "node": ">= 4"
  },
  "babel": {
    "stage": 0,
    "optional": [
      "es7.asyncFunctions",
      "es7.classProperties"
    ],
    "blacklist": [
      "es6.arrowFunctions",
      "es6.blockScoping",
      "es6.constants",
      "es6.forOf",
      "es6.properties.computed",
      "es6.properties.shorthand",
      "es6.regex.sticky",
      "es6.regex.unicode",
      "es6.tailCall",
      "es6.templateLiterals",
      "regenerator"
    ]
  }
}
